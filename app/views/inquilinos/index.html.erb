<h1>Lista de inquilinos registrados</h1>

<%= link_to ' Crear Nuevo Inquilino', "/inquilinos/crear", class:"btn btn-primary" %>
<br>
<br>
<table class="table table-hover table-bordered border-primary">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Nombre</th>
            <th scope="col">Apellido</th>
            <th scope="col">Rut</th>
            <th scope="col">Tel√©fono</th>
            <th scope="col">Oficina asignada</th>
            <th scope="col">Acciones</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <% @lista_inquilinos.each do |lista| %>
        
                <%= render 'inquilinos_registrados', inquilino_nuevo: lista %>

            <% end %>
            
        </tr>
        
    </tbody>
</table>
